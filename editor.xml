<application>
  <component name="DefaultFont">
    <option name="FONT_SIZE" value="18" />
    <option name="FONT_FAMILY" value="Source Code Pro" />
    <option name="USE_LIGATURES" value="true" />
    <option name="SECONDARY_FONT_FAMILY" value="Courier" />
  </component>
  <component name="EditorSettings">
    <option name="languageBreadcrumbsMap">
      <map>
        <entry key="JAVA" value="true" />
      </map>
    </option>
    <option name="USE_SOFT_WRAPS" value="" />
    <option name="USE_CUSTOM_SOFT_WRAP_INDENT" value="true" />
    <option name="STRIP_TRAILING_SPACES" value="Whole" />
    <option name="IS_ENSURE_NEWLINE_AT_EOF" value="true" />
    <option name="SHOW_QUICK_DOC_ON_MOUSE_OVER_ELEMENT" value="true" />
    <option name="QUICK_DOC_ON_MOUSE_OVER_DELAY_MS" value="1500" />
    <option name="SHOW_BREADCRUMBS_ABOVE" value="true" />
    <option name="IS_ALL_SOFTWRAPS_SHOWN" value="true" />
    <option name="IS_ANIMATED_SCROLLING" value="false" />
    <option name="IS_DND_ENABLED" value="false" />
    <option name="REFRAIN_FROM_SCROLLING" value="true" />
    <option name="SHOW_NOTIFICATION_AFTER_REFORMAT_CODE_ACTION" value="false" />
    <option name="SHOW_NOTIFICATION_AFTER_OPTIMIZE_IMPORTS_ACTION" value="false" />
  </component>
  <component name="JetEditorOptions">
    <option name="donTShowConversionDialog" value="true" />
    <option name="enableJavaToKotlinConversion" value="false" />
  </component>
  <component name="SpringBootEditorOptions">
    <option name="showAdditionalConfigNotification" value="false" />
  </component>
  <component name="SpringEditorOptions">
    <option name="showMultipleContextsPanel" value="false" />
    <option name="showProfilesPanel" value="false" />
  </component>
  <component name="TodoConfiguration">
    <pattern pattern="\btodo\b.*" />
    <pattern icon="important" pattern="\bfixme\b.*" />
    <pattern icon="question" pattern="\bdebug\b.*" />
  </component>
</application>